{
  "strip_report_specification": "v1.0",
  "format_definition": {
    "report_type": "real_time_test_strip",
    "target_systems": ["ios_ui", "react_dashboard", "voice_reporting", "ascii_terminal"],
    "update_frequency": "1s",
    "retention_policy": "7_days_local_30_days_supabase"
  },
  "strip_report_structure": {
    "header": {
      "test_id": "string",
      "agent_name": "string",
      "timestamp": "iso8601",
      "blockchain_hash": "string",
      "execution_status": "enum[running|passed|failed|timeout|error]"
    },
    "visual_indicators": {
      "status_icon": {
        "running": "‚óè",
        "passed": "‚úì",
        "failed": "‚úó",
        "timeout": "‚ßó",
        "error": "‚ö†"
      },
      "progress_bar": {
        "width": 30,
        "filled_char": "‚ñà",
        "empty_char": "‚ñë",
        "format": "[‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñà‚ñë‚ñë‚ñë‚ñë‚ñë‚ñë] 67.5%"
      },
      "health_meter": {
        "critical": "üî¥",
        "warning": "üü°",
        "healthy": "üü¢",
        "unknown": "‚ö™"
      }
    },
    "metrics_summary": {
      "execution_time": {
        "current": "float_seconds",
        "target": "string_with_unit",
        "status": "enum[pass|fail|warning]"
      },
      "voice_response_time": {
        "current": "float_seconds",
        "target": "string_with_unit",
        "status": "enum[pass|fail|warning]"
      },
      "tool_activation_rate": {
        "current": "percentage",
        "target": "percentage",
        "status": "enum[pass|fail|warning]"
      },
      "prism_confidence": {
        "current": "percentage",
        "target": "percentage",
        "status": "enum[pass|fail|warning]"
      }
    },
    "step_summary": {
      "total_steps": "integer",
      "completed_steps": "integer",
      "failed_steps": "integer",
      "current_step": "string",
      "estimated_completion": "iso8601_or_null"
    }
  },
  "output_formats": {
    "ascii_terminal": {
      "width": 80,
      "compact_mode": true,
      "template": "‚ïê‚ïê‚ïê {status_icon} {test_id} ‚îÇ {status} ‚îÇ {duration}s ‚îÇ {progress} ‚îÇ {health} ‚ïê‚ïê‚ïê"
    },
    "ios_ui_json": {
      "format": "json",
      "real_time_updates": true,
      "push_notifications": true,
      "fields": [
        "test_id", "status", "progress_percentage", "execution_time",
        "voice_summary", "critical_alerts", "step_details"
      ]
    },
    "react_dashboard": {
      "format": "json",
      "websocket_updates": true,
      "chart_data": true,
      "fields": [
        "test_metrics", "time_series_data", "step_timeline",
        "voice_transcripts", "blockchain_verification"
      ]
    },
    "voice_report": {
      "format": "structured_text",
      "voice_agent": "natasha_volkov",
      "summary_length": "30_seconds_max",
      "natural_language": true,
      "priority_alerts": true
    }
  },
